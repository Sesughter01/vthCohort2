}
@font-face {
    font-family: 'Lato';
    font-style: normal;
    font-weight: 400;
    src: url('lato/lato-regular.woff2') format('woff2'),
         url('lato/lato-regular.woff') format('woff')
}
@font-face {
    font-family: 'Lato';
    font-style: italic;
    font-weight: 400;
    src: url('lato/lato-italic.woff2') format('woff2'),
